link: https://leetcode.com/problems/reverse-string/description/

Example 1:

Input: s = ["h","e","l","l","o"]
Output: ["o","l","l","e","h"]
Example 2:

Input: s = ["H","a","n","n","a","h"]
Output: ["h","a","n","n","a","H"]
---------------------------------------

class Solution {
public:
    void reverseString(vector<char>& s) {

        int st=0,e=s.size()-1;

        while(st<e)
        {
            swap(s[st++],s[e--]);
        }
        
    }
};
